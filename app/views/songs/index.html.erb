<h1>Songs</h1>
<ul>
  <% @songs.all.each do |song| %>
    <% if song.artist %>
      <li><a href= "<%=song_path(song)%>"><%= song.artist_name %> - <%= song.title %></a></li>
    <% else %>
      <li><a href= "<%=song_path(song)%>"><%= "No Artist Specified" %> - <%= song.title %></a></li>
    <% end %>
  <% end %>
</ul>
